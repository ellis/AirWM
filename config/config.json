{
	"startup_applications" : [
		"xterm",
		"xterm",
		"xterm",
		"lxqt-panel"
	],
	"keybindings" : [
		{ "key": "1", "modifier": ["super"], "action": {"type": "desktop.raise", "num": 0}},
		{ "key": "2", "modifier": ["super"], "action": {"type": "desktop.raise", "num": 1}},
		{ "key": "3", "modifier": ["super"], "action": {"type": "desktop.raise", "num": 2}},
		{ "key": "4", "modifier": ["super"], "action": {"type": "desktop.raise", "num": 3}},
		{ "key": "5", "modifier": ["super"], "action": {"type": "desktop.raise", "num": 4}},
		{ "key": "6", "modifier": ["super"], "action": {"type": "desktop.raise", "num": 5}},
		{ "key": "7", "modifier": ["super"], "action": {"type": "desktop.raise", "num": 6}},
		{ "key": "8", "modifier": ["super"], "action": {"type": "desktop.raise", "num": 7}},
		{ "key": "9", "modifier": ["super"], "action": {"type": "desktop.raise", "num": 8}},
		{
			"key"      : "d",
			"modifier" : ["super"],
			"program"  : "dmenu_run"
		},
		{
			"key"      : "Return",
			"modifier" : ["super"],
			"program"  : "xterm"
		},
		{
			"key"      : "w",
			"modifier" : ["super"],
			"command"  : "CloseWindow"
		},
		{
			"key"      : "q",
			"modifier" : ["super"],
			"command"  : "Shutdown"
		},
		{
			"key"      : "t",
			"modifier" : ["super"],
			"command"  : "SwitchTilingMode"
		},
		{
			"key"      : "b",
			"modifier" : ["super"],
			"command"  : "SwitchWorkspaceLeft"
		},
		{
			"key"      : "f",
			"modifier" : ["super"],
			"command"  : "SwitchWorkspaceRight"
		},
		{
			"key"      : "j",
			"modifier" : ["super","shift"],
			"command"  : "MoveWindowNext"
		},
		{
			"key"      : "k",
			"modifier" : ["super","shift"],
			"command"  : "MoveWindowPrev"
		},
		{
			"key"      : "j",
			"modifier" : ["super"],
			"action"   : {"type": "focus.moveNext"}
		},
		{
			"key"      : "k",
			"modifier" : ["super"],
			"action"   : {"type": "focus.movePrev"}
		}
	]
}
